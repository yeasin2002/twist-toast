    "check-types": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "catalog:"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "catalog:",
    "@vitest/browser-playwright": "^4.0.16",
    "@twist-toast/typescript-config": "workspace:*",
    "playwright": "^1.57.0",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "^4.0.16",
    "vitest-browser-vue": "^2.0.1",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  }
