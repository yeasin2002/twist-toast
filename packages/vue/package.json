{
  "name": "@twist-toast/vue",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "description": "Vue adapter starter for twist-toast (scaffolding only).",
  "author": "Md Kawsar Islam Yeasin <mdkawsarislam2002@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/yeasin2002/twist-toast#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeasin2002/twist-toast.git"
  },
  "bugs": {
    "url": "https://github.com/yeasin2002/twist-toast/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "play": "vite",
